{
  "name": "@monokai/monoco",
  "version": "0.0.2",
  "author": "Monokai",
  "license": "MIT",
  "description": "custom corners and borders for html elements",
  "repository": {
    "type": "git",
    "url": "https://github.com/monokai/monoco.git"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/umd/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./source": "./src/index.ts"
  },
  "files": [
    "./dist",
    "./src"
  ],
  "browserslist": [
    ">1%, last 2 versions and > 0.05%, not dead"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:production": "rollup -c --environment BUILD:production",
    "watch": "rollup src/index.ts --format umd --name monoco --watch --output.file=dist/umd/index.js"
  },
  "keywords": [
    "corner",
    "border",
    "squircle",
    "svg",
    "html"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.24.6",
    "@clarifynl/eslint-config-clarify": "^0.14.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "rollup": "^4.18.0",
    "rollup-plugin-delete": "^2.0.0",
    "tslib": "^2.8.1"
  },
  "eslintConfig": {
    "extends": [
      "@clarifynl/clarify"
    ],
    "rules": {
      "import/extensions": 0
    }
  }
}
